{
  "name": "describe-dependencies",
  "version": "0.0.1",
  "description": "generates a document describing your project's dependencies",
  "main": "index.js",
  "bin": "./index.js",
  "author": "John Quarles",
  "license": "MIT",
  "dependencies": {
    "json2md": "^1.6.3"
  },
  "devDependencies": {
    "eslint": "^5.3.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-prettier": "^3.0.1",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "prettier": "^1.16.4"
  },
  "files": ["index.js", "lib"],
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npx describe-dependencies"
    }
  }
}
